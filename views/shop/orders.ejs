<%- include('../partials/header') %>
<main>
    <%if(orders.length <=0){ %>
        <h1>No Orders</h1> 
    <%} else { %>
        <ul>
            <%orders.forEach(order=> {%>
                <li>
                    <h1>#<%=order.id %></h1>
                    </li>
                    <ul>
                        <%order.products.forEach(product=> { %>
                            <li><%=product.title%> (<%=product.orderItem.quantity %>)</li>
                       <% }) %>
                    </ul>
           <% }) %>
         
        </ul>
   <% } %>
</main>

<%- include('../partials/footer') %>